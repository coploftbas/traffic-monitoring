        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <a href="#">
                        OD Matrix
                    </a>
                </li>
              <%= form_tag transaction_generateOD_path(:format => 'js'), remote: true, method: :get do -%>
                <li>
                  <%= text_field_tag :rangeFrom, 'Range from (click here)', class: 'datepicker'  %>
                </li>
                <li>
                    <p/>
                </li>
                <li>
                  <%= text_field_tag :rangeTo, 'Range to (click here)', class: 'datepicker'  %>
                </li>
                <li>
                    <p/>
                </li>
                <li>
                  <%= button_tag 'Submit', class: 'btn btn-mini btn-primary' %>
                </li>
              <% end %>
